@article{Epifanov_2011,
	doi = {10.1088/1742-6596/293/1/012009},
	url = {https://doi.org/10.1088%2F1742-6596%2F293%2F1%2F012009},
	year = 2011,
	month = {April},
	publisher = {{IOP} Publishing},
	volume = {293},
	pages = {012009},
	author = {Denis Epifanov},
	title = {Electromagnetic calorimeters of the {CMD}-3 detector},
	journal = {Journal of Physics: Conference Series},
	abstract = {Recently the CMD-3 detector has started to record the experimental information produced at the VEPP-2000 e+e− collider at BINP (Novosibirsk). CMD-3 is a general purpose detector for a study of e+e− annihilation to hadrons in the wide center-of-mass energy range, The calorimetry in the detector is based on three subsystems: closest to the beam pipe barrel liquid xenon calorimeter, outer barrel calorimeter based on CsI scintillation crystals and endcap calorimeter made of BGO scintillation crystals. Construction and tests of the calorimeters are described, first results from the experimental runs are presented.}
}

